{
  "name": "chatweb3",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build && cd prisma && touch dev.db && prisma generate && npm run db:migrate && npm run seed",
    "start": "next start",
    "lint": "next lint",
    "prisma-validate": "prisma validate && prisma format",
    "db:generate": "prisma generate",
    "db:migrate": "prisma db push --force-reset",
    "db-alter": "npm run prisma-validate && prisma migrate deploy",
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.14.0",
    "eth-crypto": "^2.6.0",
    "ethers": "^5.7.2",
    "next": "14.2.3",
    "prisma": "^5.14.0",
    "react": "^18",
    "react-dom": "^18",
    "react-icons": "^5.2.1",
    "react-tooltip": "^5.26.4",
    "sqlite3": "^5.1.7",
    "ts-node": "^10.9.2",
    "zod": "^3.23.8",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.3",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
