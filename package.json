{
  "name": "chatweb3",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && npm run migrate-prod && next build",
    "start": "next start",
    "postinstall": "prisma generate",
    "lint": "next lint",
    "prisma-validate": "prisma validate && prisma format",
    "migrate-dev": "npx prisma migrate dev --name init",
    "migrate-prod": "prisma migrate deploy",
    "db:generate": "prisma generate",
    "db:migrate": "prisma db push --force-reset",
    "db-alter": "npm run prisma-validate && prisma migrate deploy",
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.14.0",
    "@react-oauth/google": "^0.12.1", 
    "ethers": "^5.7.2",
    "next": "14.2.3",
    "next-auth": "^4.24.7",
    "prisma": "^5.14.0",
    "react": "^18",
    "react-chat-elements": "^12.0.14",
    "react-dom": "^18",
    "react-icons": "^5.2.1",
    "react-tooltip": "^5.26.4",
    "sqlite3": "^5.1.7",
    "ts-node": "^10.9.2",
    "zod": "^3.23.8",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.3",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
